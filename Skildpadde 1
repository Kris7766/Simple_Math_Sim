#include <ros/ros.h>
#include <std_msgs/Int8.h>

ros::Subscriber sub_Regnestykke;

void RegnestykkeCallback(const std_msgs::Int8::ConstPtr& msg_v1)
{
  ROS_INFO("I heard: [%i]", msg_v1->data);
}

int main(int argc, char **argv)
{
  ros::init(argc, argv, "Padde1");
  ros::NodeHandle n;
  sub_Regnestykke = n.subscribe("Regnestykke", 1000, RegnestykkeCallback);

  ros::spin();

  return 0;
}

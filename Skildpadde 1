#include <ros/ros.h>
#include <std_msgs/Int8.h>

ros::Subscriber sub_Regnestykke_v1;
ros::Subscriber sub_Regnestykke_v2;
ros::Subscriber sub_Regnestykke_op1;

void RegnestykkeCallback_v1(const std_msgs::Int8::ConstPtr& msg_v1)
{
  ROS_INFO("I heard: [%i]", msg_v1->data);
}

void RegnestykkeCallback_v2(const std_msgs::Int8::ConstPtr& msg_v2)
{
  ROS_INFO("I heard: [%i]", msg_v2->data);
}

void RegnestykkeCallback_op1(const std_msgs::Int8::ConstPtr& msg_op1)
{
  ROS_INFO("I heard: [%i]", msg_op1->data);
}

int main(int argc, char **argv)
{
  ros::init(argc, argv, "Padde1");
  ros::NodeHandle n;
  sub_Regnestykke_v1 = n.subscribe("Regnestykke", 1000, RegnestykkeCallback_v1);
  sub_Regnestykke_v2 = n.subscribe("Regnestykke", 1000, RegnestykkeCallback_v2);
  sub_Regnestykke_op1 = n.subscribe("Regnestykke", 1000, RegnestykkeCallback_op1);

  ros::spin();

  return 0;
}
